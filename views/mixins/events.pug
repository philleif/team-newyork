mixin events(meetings)

  section.agenda
    .container.grid-lg
      .columns
        .column.col-3
          h2 Agenda
        .columns.col-9
          - for (let meeting of meetings)
            .columns
              .column.col-7.col-sm-10.col-mx-auto
                h5
                  a(href="/event/" + meeting.id)= meeting.via + ": " + meeting.name
                h6= meeting.date
                small
                  em
                    p= meeting.location
                p= meeting.agenda
                p
                em (via: &nbsp;
                  a(href=meeting.viaLink)= meeting.via
                  | )
              .column.col-4.col-sm-10.col-mx-auto
                p.hide-sm
                  img.img-responsive(src="https://maps.googleapis.com/maps/api/staticmap?center=" + meeting.location + "&zoom=13&size=300x300&maptype=roadmap&markers=color:red%7Clabel:%7C" + meeting.location + "&key=AIzaSyDAU86NGlydy0BQelvDwDSKjGaMz4m-nxc")
